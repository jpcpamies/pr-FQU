// ====================
#llamadaCalendario
	display none
#llamadaCalendario:checked ~ footer
	width 100%
#llamadaCalendario:checked ~ .calendario 
	margin -80px 0 0 0 
#llamadaCalendario:checked ~ .contenidos 
	margin-left -100%
#llamadaCalendario:checked ~ .contenidos .calendario .listaDias 
	display block
#llamadaCalendario:checked ~ .decoracion .lineaOuter
	display block
#llamadaCalendario:checked ~ .decoracion .tituloOuter 
	display block

#llamadaCalendario:checked ~ .llamadaLabel
	background none
#llamadaCalendario:checked ~ .llamadaLabel .boton 
	color texto2
	transform rotateZ(180deg)
	-ms-transform rotateZ(180deg)
	-webkit-transform rotateZ(180deg)
#llamadaCalendario:checked ~ .llamadaTitulo
	margin-left -100%

