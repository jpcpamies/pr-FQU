// Acciones CHECKBOX
#llamadaCalendario
	display none
#llamadaCalendario:checked ~ .calendario 
	margin 0 
#llamadaCalendario:checked ~ .contenidos 
	margin 0
#llamadaCalendario:checked ~ .contenidos .calendario .listaDias 
	display block
#llamadaCalendario:checked ~ .contenidos .calendario .decoracion .lineaOuter
	display none
#llamadaCalendario:checked ~ .contenidos .calendario .tituloOuter 
	display block


// ANIMACIONES
@keyframes animacionTablet
	0%
		transform translate(-100%, -100%)
		-ms-transform translate(-100%, -100%)
		-webkit-transform translate(-100%, -100%)
	50%
		transform translate(0%, 40%)
		-ms-transform translate(0%, 40%)
		-webkit-transform translate(0%, 40%)

	100%
		transform translate(-100%, -100%)
		-ms-transform translate(-100%, -100%)
		-webkit-transform translate(-100%, -100%)